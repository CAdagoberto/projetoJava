<%- include('partials/head') %>

<body>
  <%- include('partials/navbarAdm') %>

  <section id="admin-pedidos">
    <h2 class="section-title">Pedidos Recebidos</h2>
    <table>
      <thead>
        <tr>
          <th>ID</th>
          <th>Produto</th>
          <th>Valor</th>
          <th>Recheio</th>
          <th>Ações</th>
        </tr>
      </thead>
      <tbody>
        <% produtos.forEach(produto => { %>
        <tr>
          <td><%= produto.id %></td>
          <td><%= produto.nome %></td>
          <td>R$ <%= produto.valor %></td>
          <td><%= produto.recheio %></td>
          <td>
            <div class="produtos_edit">
              <button class="btn-default">Atualizar</button>
              <button class="btn-default">Deletar</button>
            </div>
          </td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </section>

  <%- include('partials/footer') %>

</body>


<%- include('partials/headerFinal') %>
